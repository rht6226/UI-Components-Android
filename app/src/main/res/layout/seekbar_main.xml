<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/background"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SeekbarActivity">

    <TextView
        android:id="@+id/output"
        android:layout_width="271dp"
        android:layout_height="67dp"
        android:layout_marginBottom="224dp"
        android:padding="@dimen/std_pd"
        android:paddingStart="@dimen/std_pd"
        android:paddingLeft="@dimen/std_pd"
        android:paddingTop="@dimen/std_pd"
        android:paddingEnd="@dimen/std_pd"
        android:paddingRight="@dimen/std_pd"
        android:paddingBottom="@dimen/std_pd"
        android:textAlignment="center"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seekBar"
        app:layout_constraintVertical_bias="0.427" />

    <TextView
        android:id="@+id/question3"
        android:layout_width="281dp"
        android:layout_height="93dp"
        android:padding="@dimen/std_pd"
        android:paddingStart="@dimen/std_pd"
        android:paddingLeft="@dimen/std_pd"
        android:paddingTop="@dimen/std_pd"
        android:paddingEnd="@dimen/std_pd"
        android:paddingRight="@dimen/std_pd"
        android:paddingBottom="@dimen/std_pd"
        android:text="@string/pain"
        android:textAlignment="center"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.064" />

    <SeekBar
        android:id="@+id/seekBar"
        android:layout_width="339dp"
        android:layout_height="60dp"
        android:layout_marginBottom="356dp"
        android:max="10"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/question3"
        app:layout_constraintVertical_bias="0.508" />

    <ToggleButton
        android:id="@+id/toggleButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ToggleButton"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/output" />


</androidx.constraintlayout.widget.ConstraintLayout>
